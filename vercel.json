{
    "version": 2,
    "builds": [
        {
            "src": "src/index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "src/routes/$1"
        },
        {
            "src": "/(.*)",
            "dest": "src/index.js"
        }
    ]
}